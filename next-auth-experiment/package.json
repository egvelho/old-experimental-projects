    "material-icons": "^0.3.1",
    "next": "^10.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-input-mask": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
